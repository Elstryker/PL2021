int lol

def outrasCenas() {
	int talvez = 4
	printf("Funções encadeadas funcionam?\n")
	return talvez
}

def cenas() {
	int i
	int k
	int j

	while (j < 2) {
		for (i = 3 ; i > 0 ; i = i - 1) {
			printf("LI4 é merda\n")
		}
		j = j + 1
	}
	k = call outrasCenas()
	return k
}

lol = call cenas()

printf("O André curte levar de: ")
printf(lol)
